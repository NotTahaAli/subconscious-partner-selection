# LLM API Configuration
LLM_API_KEY=your_api_key_here
LLM_API_ENDPOINT=https://api.your-llm-provider.com/v1/chat/completions

# Optional Configuration
NODE_ENV=development
NEXT_PUBLIC_MAX_TRAIT_SCORE=3000  # Maximum score for trait rankings

# Example values for different LLM providers:

# OpenAI
# LLM_API_ENDPOINT=https://api.openai.com/v1/chat/completions
# LLM_API_KEY=sk-...

# Anthropic
# LLM_API_ENDPOINT=https://api.anthropic.com/v1/messages
# LLM_API_KEY=sk-ant-...